{
    "name": "laravel/laravel",
    "description": "The Laravel Framework.",
    "keywords": ["MSPRH", "Dossier Patient"],
    "license": "MIT",
    "type": "project",
    "require": {
        "php": ">=7.0.0",
        "acacha/ace-template-laravel": "0.1.4.1",
        "barryvdh/laravel-dompdf": "^0.8.1",
        "bigfish/pdf417": "^0.3.0",
        "components/font-awesome": "^5.12",
        "doctrine/dbal": "^2.5",
        "fideloper/proxy": "~3.3",
        "http-interop/http-factory-guzzle": "^1.1",
        "intercom/intercom-php": "^4.4",
        "laracasts/utilities": "^3.0",
        "laravel-validation-rules/phone": "^1.0",
        "laravel/framework": "5.5.*",
        "laravel/tinker": "~1.0",
        "league/flysystem": "^1.0",
        "mercuryseries/flashy": "^1.0",
        "mercuryseries/laravel-helpers": "^1.0",
        "milon/barcode": "^5.3",
        "symfony/http-client": "^5.4",
        "yajra/laravel-datatables-oracle": "~8.0"
    },
    "require-dev": {
        "barryvdh/laravel-debugbar": "^3.1",
        "filp/whoops": "~2.0",
        "fzaninotto/faker": "~1.4",
        "mockery/mockery": "0.9.*",
        "phpunit/phpunit": "~6.0"
    },
    "autoload": {
        "classmap": [
            "database/seeds",
            "database/factories"
        ],
        "psr-4": {
            "App\\": "app/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "dont-discover": [
            ]
        }
    },
    "scripts": {
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate"
        ],
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover"
        ]
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true,
        "github-oauth": {

        },
        "allow-plugins": {
            "php-http/discovery": true
        }
    },
    "repositories": [
    {
        "packagist": false
    },
    {
        "type": "composer",
         "no-api": true,
         "url": "https://packagist.org/"
    }
]
}
